package javaJDBCoil;

public class OilVo {
	private String saleno;
	private String oildate;
	private String oilname;
	private int amount;
	private String paytype;
	private String custno;
	private String creditcart;
	private int oilcost;
	private int count;
	private int tot;
	private String tel1;
	private String tel2;
	private String tel3;
	
	public String getSaleno() {
		return saleno;
	}
	public void setSaleno(String saleno) {
		this.saleno = saleno;
	}
	public String getOildate() {
		return oildate;
	}
	public void setOildate(String oildate) {
		this.oildate = oildate;
	}
	public String getOilname() {
		return oilname;
	}
	public void setOilname(String oilname) {
		this.oilname = oilname;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public String getPaytype() {
		return paytype;
	}
	public void setPaytype(String paytype) {
		this.paytype = paytype;
	}
	public String getCustno() {
		return custno;
	}
	public void setCustno(String custno) {
		this.custno = custno;
	}
	public String getCreditcart() {
		return creditcart;
	}
	public void setCreditcart(String creditcart) {
		this.creditcart = creditcart;
	}
	public int getOilcost() {
		return oilcost;
	}
	public void setOilcost(int oilcost) {
		this.oilcost = oilcost;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public int getTot() {
		return tot;
	}
	public void setTot(int tot) {
		this.tot = tot;
	}
	public String getTel1() {
		return tel1;
	}
	public void setTel1(String tel1) {
		this.tel1 = tel1;
	}
	public String getTel2() {
		return tel2;
	}
	public void setTel2(String tel2) {
		this.tel2 = tel2;
	}
	public String getTel3() {
		return tel3;
	}
	public void setTel3(String tel3) {
		this.tel3 = tel3;
	}
	
	
}
