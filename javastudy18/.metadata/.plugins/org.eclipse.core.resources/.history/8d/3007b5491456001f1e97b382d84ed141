package javaJDBCshop;

import java.sql.*;
import java.util.*;

public class ShopDao {
	public List<ShopVo> insertMember(List<ShopVo> list) {
		Connection conn = null;
		PreparedStatement pstmt = null;
		
		String sql = "insert into member_tbl_02 values (?, '차공단', '010-1111-7777', '제주도 제주시 외나무골', '20151211', 'C', '60')";
		
		return list;
	}
	
	private int getAuto(int custno) {
		Connection conn = null;
		PreparedStatement pstmt = null;
		ResultSet rs = null;
		
		String sql = "select max(custno)+1 as custno\r\n" + 
				"from member_tbl_02)";
		
		try {
			conn = DBManager.getInstance().getDBManager();
			pstmt = conn.prepareStatement(sql);
			rs = pstmt.executeQuery();
			if(rs.next()) {
				custno = rs.getInt("custno");
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		return custno;
	}
}
